{
  "name": "bls-wage-data-getter",
  "private": true,
  "type": "module",
  "scripts": {
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f",
    "db:migrate": "node --loader ts-node/esm scripts/apply_migrations.ts",
    "db:codegen": "kysely-codegen --dialect postgres --url postgres://oews:oews@localhost:5432/oews --out-file src/db/generated/db.d.ts",
    "db:setup": "npm run db:up && npm run db:migrate && npm run db:codegen",
    "db:test:conn": "node --loader ts-node/esm src/scripts/test_db_connection.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "get:socs": "node --loader ts-node/esm src/scripts/get_socs.ts",
    "get:naics": "node --loader ts-node/esm src/scripts/get_naics.ts",
    "crawl": "node --loader ts-node/esm src/scripts/crawl_wages.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.47",
    "kysely": "^0.28.9",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^11.11.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^22.10.6",
    "@types/pg": "^8.11.12",
    "@types/prettier": "^3.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "husky": "^9.1.7",
    "kysely-codegen": "^0.19.0",
    "nock": "^14.0.10",
    "prettier": "^3.7.4",
    "testcontainers": "^11.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vitest": "^4.0.16"
  }
}
